# Exercice 3
basket = ["Banana", "Apples", "Oranges", "Blueberries"]
basket.pop(0)
print(basket)
basket.pop(2)
print(basket)
basket.append("kiwi")
print(basket)
basket.insert(0, "Apples")
print(basket)
apple_count = basket.count("Apples")
print("Number for apple", apple_count)
basket.clear()
print(basket)

# Exercice 5

numbers = list(range(1,21))
for number in numbers:
    print(number)
for numbers in range(1, 21):
    if numbers % 2 == 0:  # Check if the index is even
        print(numbers)


